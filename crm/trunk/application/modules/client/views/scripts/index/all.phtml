<?php if (isset($this->clients)) { ?>
	<table width="100%" border="1">
	<?php $client = reset($this->clients); ?>
	<tr>
	<?php foreach ($client as $key => $field) { ?>
		<th><?php echo $key ?></th>
	<?php }	?>
	</tr>
	<?php foreach ($this->clients as $client) { ?>
		<tr>
		<?php foreach ($client as $field) { ?>
			<td><?php echo $field ?></td>
		<?php }	?>
		</tr>
	<?php } ?>
	</table>
<?php } ?>